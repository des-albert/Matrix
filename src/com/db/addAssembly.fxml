<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="root" stylesheets="@../../css/style.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.db.addAssembly">
   <children>
      <Button fx:id="ButtonAddAssemblyDone" layoutX="490.0" layoutY="159.0" mnemonicParsing="false" onAction="#ButtonAddAssemblyDoneOnAction" text="Close" />
      <Label layoutX="51.0" layoutY="40.0" styleClass="label-plain" text="Add Assembly" />
      <TextField fx:id="TextAssemblyName" layoutX="51.0" layoutY="106.0" promptText="Name" />
      <TextField fx:id="TextAssemblyCount" layoutX="51.0" layoutY="169.0" prefHeight="31.0" prefWidth="71.0" promptText="Count" />
      <Button fx:id="ButtonAddAssembly" layoutX="492.0" layoutY="106.0" mnemonicParsing="false" onAction="#ButtonAddAssemblyOnAction" text="Add " />
      <Label fx:id="LabelAddAssemblyStatus" layoutX="14.0" layoutY="345.0" prefHeight="21.0" prefWidth="568.0" />
      <RadioButton fx:id="RadioButtonMax" layoutX="183.0" layoutY="174.0" mnemonicParsing="false" selected="true" text="Maximum" userData="M">
         <toggleGroup>
            <ToggleGroup fx:id="CountGroup" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="RadioButtonExact" layoutX="183.0" layoutY="212.0" mnemonicParsing="false" prefHeight="21.0" prefWidth="93.0" text="Exact" toggleGroup="$CountGroup" userData="E" />
      <RadioButton fx:id="RadioButtonRequired" layoutX="182.0" layoutY="252.0" mnemonicParsing="false" prefHeight="21.0" prefWidth="113.0" text="Required" toggleGroup="$CountGroup" userData="R" />
      <RadioButton fx:id="RadioButtonUnlimited" layoutX="182.0" layoutY="288.0" mnemonicParsing="false" prefHeight="21.0" prefWidth="113.0" text="Unlimited" toggleGroup="$CountGroup" userData="U" />
   </children>
</AnchorPane>
